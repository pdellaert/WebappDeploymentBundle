{% extends "DellaertWebappDeploymentBundle::layout.html.twig" %}

{% block content %}
	{% if entity is defined %}
		<script type="text/javascript">
		setTimeout(function() {
			  window.location.href = "{{ path('DeploymentTypeList') }}";
			}, 3000);
		</script>
		<h1>Deployment type deleted: {{ entity.name }}</h1>
		<p>You will be returned to the deployment type list in 3 seconds...</p>
	{% else %}
		<h1>Unknown deployment type</h1>
		<p>The specified deployment type is not available.</p>
	{% endif %}
{% endblock %}
