{% extends "DellaertWebappDeploymentBundle::layout.html.twig" %}

{% block content %}
	{% if entity is defined %}
		<script type="text/javascript">
		setTimeout(function() {
			  window.location.href = "{{ path('ServerTypeList') }}";
			}, 3000);
		</script>
		<h1>Server type deleted: {{ entity.name }}</h1>
		<p>You will be returned to the server type list in 3 seconds...</p>
	{% else %}
		<h1>Unknown server type</h1>
		<p>The specified server type is not available.</p>
	{% endif %}
{% endblock %}
