{% extends "DellaertWebappDeploymentBundle::layout.html.twig" %}

{% block content %}
	{% if entity is defined %}
    	<script type="text/javascript">
    	setTimeout(function() {
    		  window.location.href = "{{ path('APIKeyList') }}";
    		}, 3000);
    	</script>
    	<h1>API Key deleted: {{ entity.name }}</h1>
    	<p>You will be returned to the API key list in 3 seconds...</p>
    {% else %}
		<h1>Unknown API key</h1>
		<p>The specified API key is not available.</p>
    {% endif %}
{% endblock %}
