{% extends "DellaertWebappDeploymentBundle::layout.html.twig" %}

{% block content %}
	{% if entity is defined %}
		<script type="text/javascript">
		setTimeout(function() {
			  window.location.href = "{{ path('DatabaseTypeList') }}";
			}, 3000);
		</script>
		<h1>Database type deleted: {{ entity.name }}</h1>
		<p>You will be returned to the database type list in 3 seconds...</p>
	{% else %}
		<h1>Unknown database type</h1>
		<p>The specified database type is not available.</p>
	{% endif %}
{% endblock %}
