{% extends "DellaertWebappDeploymentBundle::layout.html.twig" %}

{% block content %}
	{% if entity is defined %}
		<script type="text/javascript">
		setTimeout(function() {
			  window.location.href = "{{ path('ApplicationList') }}";
			}, 3000);
		</script>
		<h1>Application deleted: {{ entity.name }}</h1>
		<p>You will be returned to the application list in 3 seconds...</p>
	{% else %}
		<h1>Unknown application</h1>
		<p>The specified application is not available.</p>
	{% endif %}
{% endblock %}
